var status_element = $('.cell-data--<%= @reservation.id %>-status-update');
$(status_element).html('<%= escape_javascript render "status_update", reservation: @reservation %>');

var updated = <%= @updated %>
if (updated && $('.welcome-tiles').length) {
  var welcome_reservations = $('.tile-link.pending-reservations .tile-body');
  var count = +($(welcome_reservations).html().trim()) - 1;
  $(welcome_reservations).html(count);
}
